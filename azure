# Sample Python Program

# Function to check if a number is prime
def is_prime(n):
    if n <= 1:
        return False
    for i in range(2, int(n**0.5) + 1):
        if n % i == 0:
            return False
    return True

# Main program
print("Prime numbers between 1 and 50:")

for num in range(1, 51):
    if is_prime(num):
        print(num, end=" ")

print("\nProgram finished!")
